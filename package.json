{
  "name": "pre-fix",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "prepush-check": "npm run lint && npm run type-check && npm run format-check",
    "type-check": "tsc --noEmit",
    "format-check": "prettier --check .",
    "migrate:generate_from_schema": "drizzle-kit generate --config ./drizzle.config.ts",
    "migrate:generate": "npx drizzle-kit generate --schema=./src/db/schema.ts --out=./drizzle --dialect=postgresql --custom",
    "migrate:push": "npx tsx src/db/migrate.ts",
    "format-code": "prettier --write ."
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^5.1.1",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@mui/material-nextjs": "^7.2.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/moment": "^2.11.29",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "fast-xml-parser": "^5.2.5",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "next": "15.3.2",
    "next-auth": "^4.24.11",
    "pg": "^8.16.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
    "react-redux": "^9.2.0",
    "sass": "^1.89.2",
    "xlsx": "^0.18.5",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.18",
    "@types/node": "^20",
    "@types/pg": "^8.15.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.28.0",
    "eslint-config-next": "15.3.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
